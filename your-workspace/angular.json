{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "overview": {
      "root": "apps/overview/",
      "sourceRoot": "apps/overview/src",
      "projectType": "application",
      "prefix": "david",
      "schematics": {
        "@nrwl/schematics:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/overview",
            "index": "apps/overview/src/index.html",
            "main": "apps/overview/src/main.ts",
            "polyfills": "apps/overview/src/polyfills.ts",
            "tsConfig": "apps/overview/tsconfig.app.json",
            "assets": [
              "apps/overview/src/favicon.ico",
              "apps/overview/src/assets"
            ],
            "styles": ["apps/overview/src/styles.scss"],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/overview/src/environments/environment.ts",
                  "with": "apps/overview/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "overview:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "overview:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "overview:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/overview/tsconfig.app.json",
              "apps/overview/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/overview/src/test.ts",
            "tsConfig": "apps/overview/tsconfig.spec.json",
            "karmaConfig": "apps/overview/karma.conf.js",
            "polyfills": "apps/overview/src/polyfills.ts",
            "styles": [],
            "scripts": [],
            "assets": []
          }
        }
      }
    },
    "overview-e2e": {
      "root": "apps/overview-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/overview-e2e/protractor.conf.js",
            "devServerTarget": "overview:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "overview:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/overview-e2e/tsconfig.e2e.json",
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
